<div class="booyah-box col-10 offset-1">
  <h1>Edit Your Product</h1>
  <%= simple_form_for @prod do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :cost %>
    <%= f.input :images, input_html: { multiple: true }, as: :file %>
    <br />
    <%= f.submit 'Update', class: 'btn btn-outline-primary' %>
  <% end %>
</div>